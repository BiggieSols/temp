<!doctype html>
<!--[if lt IE 7]> <html class="no-js lt-ie10 lt-ie9 lt-ie8 lt-ie7" lang="en-us" xmlns:fb="http://ogp.me/ns/fb#"> <![endif]-->
<!--[if IE 7]> <html class="no-js lt-ie10 lt-ie9 lt-ie8" lang="en-us" xmlns:fb="http://ogp.me/ns/fb#"> <![endif]-->
<!--[if IE 8]> <html class="no-js lt-ie10 lt-ie9" lang="en-us" xmlns:fb="http://ogp.me/ns/fb#"> <![endif]-->
<!--[if IE 9]> <html class="no-js lt-ie10" lang="en-us" xmlns:fb="http://ogp.me/ns/fb#"> <![endif]-->
<!--[if gt IE 9]><!--> <html class="no-js" lang="en-us" xmlns:fb="http://ogp.me/ns/fb#"> <!--<![endif]-->
  <head>
    <meta charset="utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
    <title></title>

    <meta name="viewport" content="width=device-width"/>
    <meta property="fb:app_id" content=""/>
    <base target="_blank" />

    {{>styles}}

    <!--[if lt IE 9]>
    <script src="{{staticPath "js/html5-shiv.js"}}"></script>
    <script src="{{staticPath "js/css3-mediaqueries.js"}}"></script>
    <![endif]-->
  </head>
  <body>

    <div class="pcmax-section">
      <div class="pcmax-tow">
        <div class="head-container">
          <h1 class="header">{{settings.header.headerText}}</h1>
        </div>

        <h2 class="question">{{settings.header.questionText}}</h2>

        <div class="progress-container">
          <div class="progress-bar">
            <div class="progress-percent left" style="width: 50%"></div>
            <div class="progress-percent right" style="width: 50%"></div>
          </div>
          <div class="percent-left pct">50%</div>
          <div class="percent-right pct">50%</div>
        </div>

        <div class="vote-mssg">{{settings.header.voteLabel}}</div>

        {{component "poll-content"}}

      </div>
    </div>

    {{>massrel}}

    
    <!--[if IE]>
      <script>
        $('html').addClass('ie');
      </script>
    <![endif]-->

    <script src="{{staticPath "js/index.js"}}"></script>
    <script>{{#minify}}
    (function (ns) {
      window.MR_ready = function (components) {
        var done = this.async();
        ns.require(['js/index'], function (index) {
          index.ready(components);
          done();
        });
      };
      window.MR_render = function (components) {
        ns.require(['js/index'], function (index) {
          index.render(components);
        });
      };
    }(window.MRT || window));
    {{/minify}}</script>

  </body>
</html>
